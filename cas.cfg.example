
[CAS]
lt_lifespan = 300
st_lifespan = 10
pt_lifespan = 10
pgt_lifespan = 600
tgt_lifespan = 86400
validate_pgturl = 0
ticket_size = 256

[PLUGINS]
cred_checker = LDAPSimpleBindChecker
realm = LDAPRealm
ticket_store = CouchDBTicketStore

[InMemoryTicketStore]
verify_cert = True

[LDAP]
